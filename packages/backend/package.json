{
  "name": "backend-boilerplate",
  "version": "1.1.0",
  "license": "MIT",
  "repository": "github:PeterSkriba/boilerplate",
  "author": "PeterSkriba <peter.skriba@gmail.com> (https://github.com/PeterSkriba)",
  "homepage": "https://github.com/PeterSkriba/boilerplate#readme",
  "description": "TypeScript, Prisma, GraphQL, PostgreSQL backend boilerplate",
  "keywords": [
    "nodejs",
    "typescript",
    "prisma",
    "graphql",
    "postgresql",
    "boilerplate"
  ],
  "scripts": {
    "start": "nodemon -r dotenv/config",
    "studio": "npx prisma studio",
    "database:migrate": "npx prisma migrate dev --name init --preview-feature",
    "database:deploy": "npx prisma migrate deploy --preview-feature",
    "generate": "npx prisma generate",
    "seed": "node prisma/seed"
  },
  "dependencies": {
    "@babel/core": "^7.16.5",
    "@babel/node": "^7.16.5",
    "@babel/plugin-proposal-class-properties": "^7.16.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.16.5",
    "@babel/preset-env": "^7.16.5",
    "@babel/preset-typescript": "^7.16.5",
    "@graphql-tools/schema": "^8.3.1",
    "@nexus/schema": "^0.20.1",
    "@prisma/client": "^3.6.0",
    "apollo-server": "^3.5.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "dotenv": "^10.0.0",
    "graphql": "^16.2.0",
    "graphql-import": "^1.0.2",
    "nexus": "^1.1.0",
    "nexus-prisma": "^0.35.0",
    "nodemon": "^2.0.15",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4"
  },
  "devDependencies": {
    "@types/node": "^17.0.1",
    "prisma": "^3.6.0"
  }
}
