// SETTINGS
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DB_URL")
}

// MODELS
model User {
  id         String   @id @default(uuid())
  email      String   @unique
  password   String
  first_name String?
  last_name  String?
  createdAt  DateTime @default(now())
}
