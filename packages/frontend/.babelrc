{
  "presets": [
    [
      "@babel/preset-env",
      {
        "useBuiltIns": "entry",
        "corejs": 3,
        "targets": {
          "browsers": ["last 2 versions", "safari >= 7"]
        }
      }
    ],
    "@babel/preset-react",
    "@babel/preset-typescript"
  ],

  "plugins": ["@babel/plugin-transform-runtime"]
}
